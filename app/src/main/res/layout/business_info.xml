<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:cardElevation="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="end|center_vertical"
            android:layout_marginEnd="20dp"
            android:src="@drawable/more_vert" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Business Address"
            android:textColor="@color/black"
            android:textSize="24sp" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Business Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.197"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_image_1"
        app:layout_constraintVertical_bias="0.048" />

    <EditText
        android:id="@+id/state_ed"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/input_field"
        android:hint="business name"
        android:padding="7dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/district_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="32dp"
        android:text="Business Type"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/state_ed" />

    <EditText
        android:id="@+id/district_ed"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/input_field"
        android:hint="business type"
        android:padding="7dp"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/state_ed"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/state_ed"
        app:layout_constraintTop_toBottomOf="@+id/district_txt" />

    <TextView
        android:id="@+id/pin_code_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Business Since"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/district_txt"
        app:layout_constraintTop_toBottomOf="@+id/district_ed" />

    <EditText
        android:id="@+id/pincode_ed"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/input_field"
        android:hint="business since"
        android:padding="7dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/district_ed"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/district_ed"
        app:layout_constraintTop_toBottomOf="@+id/pin_code_txt" />

    <TextView
        android:id="@+id/landmark_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Timing"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/pin_code_txt"
        app:layout_constraintTop_toBottomOf="@+id/pincode_ed" />


    <EditText
        android:id="@+id/landmark_ed"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/input_field"
        android:hint="timing"
        android:padding="7dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/pincode_ed"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/pincode_ed"
        app:layout_constraintTop_toBottomOf="@+id/landmark_txt" />

    <TextView
        android:id="@+id/pricing_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Pricing"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/landmark_txt"
        app:layout_constraintTop_toBottomOf="@+id/landmark_ed" />

    <EditText
        android:id="@+id/pricing_ed"
        android:layout_width="318dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/input_field"
        android:hint="pricing"
        android:padding="7dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/landmark_ed"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/landmark_ed"
        app:layout_constraintTop_toBottomOf="@+id/pricing_txt" />

    <Button
        android:id="@+id/business_info_btn"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/blue"
        android:text="Next"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/pricing_ed"
        app:layout_constraintStart_toStartOf="@+id/pricing_ed"
        app:layout_constraintTop_toBottomOf="@+id/pricing_ed"
        app:layout_constraintVertical_bias="0.32" />

    <ImageView
        android:id="@+id/add_image_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toStartOf="@+id/add_image_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2"
        app:srcCompat="@drawable/add_picture_btn" />

    <ImageView
        android:id="@+id/add_image_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="172dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toStartOf="@+id/add_image_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/add_image_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2"
        app:srcCompat="@drawable/add_picture_btn" />

    <ImageView
        android:id="@+id/add_image_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/add_image_2"
        app:layout_constraintTop_toBottomOf="@+id/cardView2"
        app:srcCompat="@drawable/add_picture_btn" />

</androidx.constraintlayout.widget.ConstraintLayout>